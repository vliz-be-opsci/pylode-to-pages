PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX dc: <http://purl.org/dc/elements/1.1/>

{% for row in sets['_']%}
<{{vars_dict.baseuri}}/{{vars_dict.relref}}#{{row.PREFLABEL_EN}}>
        rdf:type               skos:Concept ;
        dc:date                "{{row.DATE}}" ;
        dc:identifier          {{row.PREFLABEL_EN|ttl("@und")}} ;
        skos:prefLabel         {{row.PREFLABEL_EN|ttl("@en")}} ;
        {%-if '[' in  row.BROADER -%}
        {%-set BROADER = row.BROADER.replace('[','').replace(']','').split(';')%}
        skos:broader           {%for B in BROADER-%} 
                                        <{{vars_dict.baseuri}}/{{vars_dict.relref}}#{{B}}> 
                                        {%- if not loop.last -%}
                                        ,                  
                                        {%- else -%}
                                        {%- endif -%}
                                {%endfor%};
        {%-endif%}
        {%-if '[' in row.NARROWER -%}
        {%-set NARROWER = row.NARROWER.replace('[','').replace(']','').split(';')%}
        skos:narrower          {%for N in NARROWER-%} 
                                <{{vars_dict.baseuri}}/{{vars_dict.relref}}#{{N}}>
                                {%- if not loop.last -%}
                                ,                  
                                {%- else -%}
                                {%- endif -%}
                                {%endfor%};
        {%-endif%}
        {%if row.DEFINITION_EN-%}
        skos:definition        {{row.DEFINITION_EN|ttl("@en")}} ;
        {%-endif%}
        {%if row.ALTLABEL_EN-%}
        skos:altLabel          {{row.ALTLABEL_EN|ttl("@en")}}
        {%-endif%}
        .
{% endfor %}
